{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "FLASK_ENV": "production",
        "FLASK_APP": "app.py",
        "POSTGRES_PRISMA_URL": "postgres://postgres:060912Wjt@db.kbbpkicqzobcrbhhcrzz.supabase.co:5432/postgres?sslmode=require",
        "POSTGRES_URL_NON_POOLING": "postgres://postgres:060912Wjt@db.kbbpkicqzobcrbhhcrzz.supabase.co:5432/postgres?sslmode=require"
    },
    "functions": {
        "app.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "regions": ["hkg1"],
    "github": {
        "enabled": true,
        "silent": false,
        "autoAlias": true
    }
} 