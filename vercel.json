{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "pythonVersion": "3.9",
                "installCommand": "pip install -r requirements.txt --no-deps && pip install psycopg2-binary --no-binary psycopg2-binary"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
} 